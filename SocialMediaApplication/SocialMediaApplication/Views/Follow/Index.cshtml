@model IEnumerable<SocialMediaApplication.Models.User>


<!-- this is a list sample of all the posts -->
@if (Model != null)
{
  <div class="container">
    @foreach (var post in Model)
        {
            if (post != null)
            {
                @Html.Partial("Components/ProfileCard",post)
            }
        }
  </div>
   
}
else
{
    <p>No posts available.</p>
}